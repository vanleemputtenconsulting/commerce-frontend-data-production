em Gummiband befestigt ist.\r\n\r\nSpezifikationen:\r\nFarbe: aqua, gold\r\nGeschlecht: Damen\r\nMaterial Anzug: Polyester\r\nMaterial Diadem: EVA\r\nGröße: S"},"brand":{"name":"Rubie's","description":null,"active":true,"logo":null,"createdAt":1651367116714,"updatedAt":1651367116714},"vendorPrice":40.78,"sellingPrice":79.95,"catalogPrice":75.02,"sellingPriceMin":null,"active":true,"featured":false,"hasVariants":false,"reviewPending":true,"categories":[{"code":"verkleedpakken","name":{"nl":"Verkleedpakken","en":"Costumes","de":"Anzug anziehen"},"slug":{"nl":"/c/verkleedpakken","en":"/en/c/costumes","de":"/de/c/anzug-anziehen"},"googleCategory":null,"parentCategory":"verkleedkleding","indexable":true,"active":true,"icon":null,"facets":null,"createdAt":1651366941540,"updatedAt":1651366941540},{"code":"verkleedkleding","name":{"nl":"Verkleedkleding","en":"Dress up clothes","de":"Kleider anziehen"},"slug":{"nl":"/c/verkleedkleding","en":"/en/c/dress-up-clothes","de":"/de/c/kleider-anziehen"},"googleCategory":null,"parentCategory":"verkleden","indexable":true,"active":true,"icon":null,"facets":null,"createdAt":1651366918738,"updatedAt":1651366918738}],"variantOptions":null,"stock":[{"id":"626def914df724026bc2dd02","location":"TWM","quantity":1,"updatedAt":1651371921063}],"images":[{"url":"https://www.twm-bv.com/producten/original/rubies_kostuum_mera_deluxe_dames_polyester_aqua_goud_mt_l_944133_1625581504.jpg"}],"attributes":[{"type":{"code":"target-audience","name":{"nl":"Doelgroep","en":"Target audience","de":"Zielgruppe"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Volwassenen","en":"Adult","de":"Erwachsene"},"variants":null,"active":true},{"type":{"code":"product-language","name":{"nl":"Producttaal","en":"Product language","de":"Produktsprache"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Geen taal","en":"No language","de":"Keine Sprache"},"variants":null,"active":true},{"type":{"code":"kind-of-dress-up-clothing","name":{"nl":"Type verkl