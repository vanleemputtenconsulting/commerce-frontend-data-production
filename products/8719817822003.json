nem Weg herum.\r\n\r\nSpezifikationen:\r\nFarbe: blau\r\nGeschlecht: Jungen\r\nMaterial: Kunststoff\r\nAbmessungen: 9,9 x 3,3 x 2,7 cm (lxbxh)\r\nSpannungsversorgung: 2x AG13 / LR44-Batterien (im Lieferumfang enthalten)\r\nInfrarotsensor: ja\r\nAlter: ab 8 Jahren"},"brand":{"name":"Hexbug","description":null,"active":true,"logo":null,"createdAt":1651368291329,"updatedAt":1651368291329},"vendorPrice":7.13,"sellingPrice":14.95,"catalogPrice":10.648,"sellingPriceMin":null,"active":true,"featured":false,"hasVariants":false,"reviewPending":true,"categories":[{"code":"robots","name":{"nl":"Robots","en":"Robots","de":"Robots"},"slug":{"nl":"/c/robots","en":"/en/c/robots","de":"/de/c/robots"},"googleCategory":null,"parentCategory":"speelfiguren-sets","indexable":true,"active":true,"icon":null,"facets":null,"createdAt":1651366973646,"updatedAt":1651366973646}],"variantOptions":null,"stock":[{"id":"626de1634df724026bc2124b","location":"TWM","quantity":4,"updatedAt":1655910936400}],"images":[{"url":"https://www.twm-bv.com/producten/original/hexbug_actievoertuig_larva_jongens_99_x_33_x_27_cm_blauw_519120_1608795164.jpg"}],"attributes":[{"type":{"code":"target-audience","name":{"nl":"Doelgroep","en":"Target audience","de":"Zielgruppe"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Kinderen","en":"Children","de":"Kind"},"variants":null,"active":true},{"type":{"code":"mobile-data-connection-possible","name":{"nl":"Mobiele data verbinding mogelijk","en":"Mobile data connection possible","de":"Mobile Datenverbindung verfügbar"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Niet van toepassing","en":"Does not apply","de":"Gilt nicht"},"variants":null,"active":true},{"type":{"code":"gender","name":{"nl":"Geslacht","en":"Gender","de":"Geschlecht"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Jongens","en":"Boys","de":"Jungen"},"variants":null,"active":true},{"type":{"code":"weight-in-kg","name":{"nl":"Gewicht in kg","en":"Weight in kg","de":"G