fel\r\n1 Würfel"},"brand":{"name":"beleduc","description":null,"active":true,"logo":null,"createdAt":1651367071643,"updatedAt":1651367071643},"vendorPrice":68.99,"sellingPrice":99.95,"catalogPrice":96.2555,"sellingPriceMin":null,"active":true,"featured":false,"hasVariants":false,"reviewPending":true,"categories":[{"code":"behendigheidsspellen","name":{"nl":"Behendigheidsspellen","en":"Patience games","de":"Patience Spiele"},"slug":{"nl":"/c/behendigheidsspellen","en":"/en/c/patience-games","de":"/de/c/patience-spiele"},"googleCategory":null,"parentCategory":"spellen","indexable":true,"active":true,"icon":null,"facets":null,"createdAt":1651366807178,"updatedAt":1651366807178},{"code":"spellen","name":{"nl":"Spellen","en":"Games","de":"Spiele"},"slug":{"nl":"/c/spellen","en":"/en/c/games","de":"/de/c/spiele"},"googleCategory":null,"parentCategory":null,"indexable":false,"active":true,"icon":null,"facets":null,"createdAt":1651366806915,"updatedAt":1651366806915}],"variantOptions":null,"stock":[{"id":"626ddcb94df724026bc1c7bc","location":"TWM","quantity":5,"updatedAt":1651367097111}],"images":[{"url":"https://www.twm-bv.com/producten/original/beleduc_bouwspel_torreta_xxl_27-delig_303275_1562161215.jpg"},{"url":"https://www.twm-bv.com/producten/original/beleduc_bouwspel_torreta_xxl_27-delig_2_303275_1562161218.jpg"},{"url":"https://www.twm-bv.com/producten/original/beleduc_bouwspel_torreta_xxl_27-delig_3_303275_1562161221.jpg"},{"url":"https://www.twm-bv.com/producten/original/beleduc_bouwspel_torreta_xxl_27-delig_4_303275_1562161224.jpg"}],"attributes":[{"type":{"code":"target-audience","name":{"nl":"Doelgroep","en":"Target audience","de":"Zielgruppe"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Kinderen","en":"Children","de":"Kind"},"variants":null,"active":true},{"type":{"code":"battery-type","name":{"nl":"Type batterij","en":"Battery type","de":"Batterietyp"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Geen batterijen","en":"No batteri