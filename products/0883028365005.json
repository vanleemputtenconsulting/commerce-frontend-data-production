ugen nicht sichtbar sind. Die Overalls werden über die Schuhe angezogen.\r\n\r\nSpezifikationen:\r\nFarbe: schwarz\r\nGeschlecht: Männer\r\nMaterial: 100 % Polyester\r\nGröße: M/L"},"brand":{"name":"Rubie's","description":null,"active":true,"logo":null,"createdAt":1651367116714,"updatedAt":1651367116714},"vendorPrice":48.6,"sellingPrice":89.95,"catalogPrice":87.7855,"sellingPriceMin":null,"active":true,"featured":false,"hasVariants":false,"reviewPending":true,"categories":[{"code":"verkleedpakken","name":{"nl":"Verkleedpakken","en":"Costumes","de":"Anzug anziehen"},"slug":{"nl":"/c/verkleedpakken","en":"/en/c/costumes","de":"/de/c/anzug-anziehen"},"googleCategory":null,"parentCategory":"verkleedkleding","indexable":true,"active":true,"icon":null,"facets":null,"createdAt":1651366941540,"updatedAt":1651366941540},{"code":"verkleedkleding","name":{"nl":"Verkleedkleding","en":"Dress up clothes","de":"Kleider anziehen"},"slug":{"nl":"/c/verkleedkleding","en":"/en/c/dress-up-clothes","de":"/de/c/kleider-anziehen"},"googleCategory":null,"parentCategory":"verkleden","indexable":true,"active":true,"icon":null,"facets":null,"createdAt":1651366918738,"updatedAt":1651366918738}],"variantOptions":null,"stock":[{"id":"626ded4a4df724026bc2c925","location":"TWM","quantity":0,"updatedAt":1654991853456}],"images":[{"url":"https://www.twm-bv.com/producten/original/rubies_verkleedkleding_kylo_ren_deluxe_star_wars_heren_pe_m_l_555356_1614248464.jpg"}],"attributes":[{"type":{"code":"target-audience","name":{"nl":"Doelgroep","en":"Target audience","de":"Zielgruppe"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Volwassenen","en":"Adult","de":"Erwachsene"},"variants":null,"active":true},{"type":{"code":"kind-of-dress-up-clothing","name":{"nl":"Type verkleedkleding","en":"Kind of Dress Up Clothing","de":"Art der Ankleidekleidung"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Volledig verkleedpak","en":"Full costume","de":"Volles Kostüm"},"variants":null,"active"