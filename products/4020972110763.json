en in die Garage und wartet auf seinen nächsten Einsatz.\r\n\r\nSpezifikationen:\r\nGeschlecht: junior\r\nFarbe: rot\r\nMaterial: Holz\r\nAbmessungen: 13 x 7 x 10 cm\r\nAlter: ab 4 Jahren"},"brand":{"name":"Small Foot","description":null,"active":true,"logo":null,"createdAt":1651366816836,"updatedAt":1651366816836},"vendorPrice":5.16,"sellingPrice":9.95,"catalogPrice":7.26,"sellingPriceMin":null,"active":true,"featured":false,"hasVariants":false,"reviewPending":true,"categories":[{"code":"hulpdiensten","name":{"nl":"Hulpdiensten","en":"Emergency","de":"Notfall"},"slug":{"nl":"/c/hulpdiensten","en":"/en/c/emergency","de":"/de/c/notfall"},"googleCategory":null,"parentCategory":"voertuigen","indexable":true,"active":true,"icon":null,"facets":null,"createdAt":1651366807099,"updatedAt":1651366807099}],"variantOptions":null,"stock":[{"id":"626dddd14df724026bc1e61a","location":"TWM","quantity":1,"updatedAt":1655334472317}],"images":[{"url":"https://www.twm-bv.com/producten/original/small_foot_ambulance_rood_13_x_7_x_10_cm_373191_1585228648.jpg"},{"url":"https://www.twm-bv.com/producten/original/small_foot_ambulance_rood_13_x_7_x_10_cm_2_373191_1585228648.jpg"},{"url":"https://www.twm-bv.com/producten/original/small_foot_ambulance_rood_13_x_7_x_10_cm_3_373191_1585228649.jpg"},{"url":"https://www.twm-bv.com/producten/original/small_foot_ambulance_rood_13_x_7_x_10_cm_4_373191_1585228650.jpg"},{"url":"https://www.twm-bv.com/producten/original/small_foot_ambulance_rood_13_x_7_x_10_cm_5_373191_1585228650.jpg"}],"attributes":[{"type":{"code":"ce-marking","name":{"nl":"CE-markering","en":"CE marking","de":"CE Kennzeichnung"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Ja","en":"Yes","de":"Ja"},"variants":null,"active":true},{"type":{"code":"target-audience","name":{"nl":"Doelgroep","en":"Target audience","de":"Zielgruppe"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Kinderen","en":"Children","de":"Kind"},"variants":null,"active":true},{"type":{"code