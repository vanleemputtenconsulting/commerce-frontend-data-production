ial: Polyresin\r\nAbmessungen: 135 x 135 x 240 cm (L x B x H)"},"brand":{"name":"CF label","description":null,"active":true,"logo":null,"createdAt":1651370847460,"updatedAt":1651370847460},"vendorPrice":143.2,"sellingPrice":214.95,"catalogPrice":210.6005,"sellingPriceMin":null,"active":true,"featured":false,"hasVariants":false,"reviewPending":true,"categories":[{"code":"kunstkerstbomen","name":{"nl":"Kunstkerstbomen","en":"Christmas trees","de":"Weihnachtsbäume"},"slug":{"nl":"/c/kunstkerstbomen","en":"/en/c/christmas-trees","de":"/de/c/weihnachtsbaeume"},"googleCategory":null,"parentCategory":"kerst","indexable":true,"active":true,"icon":null,"facets":null,"createdAt":1651370847459,"updatedAt":1651370847459}],"variantOptions":null,"stock":[{"id":"626deb614df724026bc29d3b","location":"TWM","quantity":1,"updatedAt":1655509785899}],"related":[],"images":[{"url":"https://www.twm-bv.com/producten/original/cf_label_kunstkerstboom_met_sneeuw_polyresin_240_cm_groen_461294_1600162930.jpg"}],"attributes":[{"type":{"code":"flash-function","name":{"nl":"Knipperfunctie","en":"Flash function","de":"Blitzfunktion"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Nee","en":"No","de":"Nein"},"variants":null,"active":true},{"type":{"code":"product-language","name":{"nl":"Producttaal","en":"Product language","de":"Produktsprache"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Geen taal","en":"No language","de":"Keine Sprache"},"variants":null,"active":true},{"type":{"code":"material","name":{"nl":"Materiaal","en":"Material","de":"Material"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Polyresin","en":"Polyresin","de":"Polyresin"},"variants":null,"active":true},{"type":{"code":"with-snow-decoration","name":{"nl":"Met sneeuw","en":"With snow decoration","de":"Mit Schneedekoration"},"postfix":null,"variable":true,"facet":true},"value":{"nl":"Ja","en":"Yes","de":"Ja"},"variants":null,"active":true},{"type":{"code":"indoor-or-outdoor","name":{"nl":